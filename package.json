{
  "name": "your-broker",
  "version": "1.0.0",
  "description": "Your Broker",
  "main": "build/server.js",
  "scripts": {
    "test": "mocha --require ts-node/register 'tests/**/*.ts'",
    "test:coverage": "nyc mocha --require ts-node/register 'tests/**/*.ts'",
    "db:re-migrate": "tsc && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "db:reset": "tsc && sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json .",
    "dev": "tsnd --respawn src/server.ts",
    "start": "tsc --project tsconfig.build.json && node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leobmend/your-broker.git"
  },
  "author": "leobmend",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leobmend/your-broker/issues"
  },
  "homepage": "https://github.com/leobmend/your-broker#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.4",
    "@types/sequelize": "^4.28.14",
    "@types/sinon": "^10.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.2.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "mocha": "^10.0.0",
    "mysql2": "^2.3.3",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.4.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
